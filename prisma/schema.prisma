generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Project {
  id          String   @id @default(uuid())
  name        String
  htmlContent String
  prompt      String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
